{
    "manifest_version": 3,
    "name": "CraftFeeds",
    "description": "chrome extension",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "js/background.js"
    },
    "host_permissions": [
        "http://*/",
        "https://*/"
    ]
}